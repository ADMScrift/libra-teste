FPSTab:Button{
	Name = "Remover Texturas",
	Description = nil,
	Callback = function()
        for i,v in next, (workspace:GetDescendants()) do             if v:IsA("Part") then v.Material = Enum.Material.SmoothPlastic             end             end
    end
